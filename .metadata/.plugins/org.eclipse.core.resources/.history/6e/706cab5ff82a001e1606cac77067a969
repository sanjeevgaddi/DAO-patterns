
public class SubArray {

	public static void main(String[] args) {
		int arr[]= {2,4,6,8,10};
		
		for(int i=0;i<arr.length;i++)
		{
			int start=i;
			for(int j=start;j<arr.length;j++)
			{
				System.out.print(arr[j]+" ");
			}
			System.out.println();
		}
		for(int i=1;i<arr.length;i++)
		{
			int start=i;
			for(int j=start;j<arr.length;j++)
			{
				System.out.print(arr[j]+" ");
			}
			System.out.println();
		}
		
	}

}
