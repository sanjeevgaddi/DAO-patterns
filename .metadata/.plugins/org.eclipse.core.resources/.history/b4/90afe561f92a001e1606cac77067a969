
public class SubArray {

	public static void main(String[] args) {
		int arr[]= {2,4,6,8,10};
		int length=arr.length;
		
		for(int k=0;k<arr.length;k++)
		{
		for(int i=0;i<arr.length;i++)
		{
			for(int j=0;j<=i;j++)
			{
				System.out.print(arr[j]+" ");
			}
			System.out.println();
		}
		}
		
	}

}
